public class MyClass {
    public static void main(String args[]) {
      int start=0;
      
      for(int i=0;i<4;i++) {
          int count=0;
          for(int j=1;j<=i+1;j++) {
              System.out.print(j+start+" ");
              count=j;
          }
          System.out.println();
          start+=count;
      }
    }
}
